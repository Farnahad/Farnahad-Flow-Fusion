<layout:FfAutoLayoutItem
    x:Class="FarnahadFlowFusion.Core.Control.ComboBoxLayoutItem.FfMultiSearchableComboBoxLayoutItem"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:layout="clr-namespace:FarnahadFlowFusion.Core.Control.Layout"
    xmlns:comboBox="clr-namespace:FarnahadFlowFusion.Core.Control.ComboBox"
    xmlns:ff="FarnahadFlowFusion" mc:Ignorable="d" d:DesignWidth="800"
    x:Name="MainFfAutoLayoutItem">
    <comboBox:FfComboBoxEdit ItemsSource="{ff:FfBinding Path=ItemsSource, ElementName=MainFfAutoLayoutItem}"
                              EditValue="{ff:FfBinding Path=EditValue, ElementName=MainFfAutoLayoutItem}" />
</layout:FfAutoLayoutItem>