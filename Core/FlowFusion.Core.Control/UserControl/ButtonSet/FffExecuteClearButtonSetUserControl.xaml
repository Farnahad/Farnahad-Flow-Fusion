<userControl:FfUserControl
    x:Class="FlowFusion.Core.Control.UserControl.ButtonSet.FfExecuteClearButtonSetUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:userControl="clr-namespace:FlowFusion.Core.Control.UserControl"
    xmlns:container="clr-namespace:FlowFusion.Core.Control.Container"
    xmlns:button="clr-namespace:FlowFusion.Core.Control.Button"
    xmlns:ff="FlowFusion">
    <container:FfWrapPanel HorizontalAlignment="Center">
        <button:FfSimpleButton Content="Execute" Command="{ff:FfBinding Path=ExecuteCommand}" />
        <button:FfSimpleButton Content="Clear" Command="{ff:FfBinding Path=ClearCommand}" />
    </container:FfWrapPanel>
</userControl:FfUserControl>