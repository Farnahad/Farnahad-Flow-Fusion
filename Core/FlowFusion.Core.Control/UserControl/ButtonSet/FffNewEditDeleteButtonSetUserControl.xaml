<userControl:FfUserControl
    x:Class="FarnahadFlowFusion.Core.Control.UserControl.ButtonSet.FfNewEditDeleteButtonSetUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:userControl="clr-namespace:FarnahadFlowFusion.Core.Control.UserControl"
    xmlns:container="clr-namespace:FarnahadFlowFusion.Core.Control.Container"
    xmlns:button="clr-namespace:FarnahadFlowFusion.Core.Control.Button"
    xmlns:ff="FarnahadFlowFusion">
    <container:FfWrapPanel>
        <button:FfSimpleNewButton Command="{ff:FfBinding Path=NewCommand}" />
        <button:FfSimpleEditButton Command="{Binding Path=EditCommand}" />
        <button:FfSimpleDeleteButton Command="{ff:FfBinding Path=DeleteCommand}" />
    </container:FfWrapPanel>
</userControl:FfUserControl>